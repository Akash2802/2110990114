#include <iostream>
using namespace std;

class student
{
    public:
    int rollno;
    void func(){
        cout<<"This function declared inside the class"<<endl;
    }
    void func2();
};

void student::func2(){
    cout<<"This function declared outside the class ";
}

int main(){

    student akash;
    akash.rollno=114;
    cout<<"rollno of akash is "<<akash.rollno<<endl;
    akash.func();
    akash.func2();

    return 0;
}
